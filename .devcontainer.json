{
    "name": "textual-code",
    "dockerComposeFile": "docker-compose.dev.yml",
    "service": "app",
    "workspaceFolder": "/app",
    "remoteUser": "appuser",
    "customizations": {
        "vscode": {
            "extensions": [
                "charliermarsh.ruff",
                "esbenp.prettier-vscode",
                "GitHub.copilot",
                "mhutchie.git-graph",
                "ms-python.python",
                "Textualize.textual-syntax-highlighter"
            ]
        }
    }
}